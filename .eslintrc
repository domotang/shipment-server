{
  "extends": [
    "standard",
    "prettier",
    "eslint:recommended",
    "plugin:jest/recommended"
  ],
  "rules": {
    "no-console": 1
  },
  "parser": "babel-eslint",
  "plugins": ["prettier", "jest"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  }
}
